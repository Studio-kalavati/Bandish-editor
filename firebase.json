{
  "hosting": {
    "public": "resources/public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/view/*",
        "destination": "/index.html"
    }],
      "redirects": [
          {
          "source": "/sounds/*",
          "destination": "https://storage.cloud.google.com/swaranotebook_media/sounds/",
          "type": 301
      }]
   }
}
